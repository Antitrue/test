    "@types/react-router-dom": "^5.3.3",
    "antd": "^5.20.1",
    "classnames": "2.5.1",
    "date-fns": "^3.6.0",
    "lodash": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.51.5",
    "react-redux": "9.1.2",
    "react-router-dom": "6.26.1"
