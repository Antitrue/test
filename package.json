  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "jest",
    "lint": "eslint .",
    "preview": "vite preview",
    "lint:fix": "eslint . --fix",
